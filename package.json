{
  "name": "@mndrk/agx",
  "version": "1.4.18",
  "description": "Autonomous AI Agent Orchestrator for Claude, Gemini, and Ollama",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./cli": "./index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "agx": "./index.js"
  },
  "files": [
    "index.js",
    "lib/**",
    "commands/**",
    "templates/**",
    "cloud-runtime/**"
  ],
  "scripts": {
    "prepack": "node ./scripts/package-board-runtime.js",
    "board:bundle": "node ./scripts/package-board-runtime.js",
    "test": "node --localstorage-file=/tmp/agx-jest-localstorage ./node_modules/jest/bin/jest.js --forceExit",
    "test:watch": "node --localstorage-file=/tmp/agx-jest-localstorage ./node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --localstorage-file=/tmp/agx-jest-localstorage ./node_modules/jest/bin/jest.js --coverage --forceExit",
    "test:unit": "node --localstorage-file=/tmp/agx-jest-localstorage ./node_modules/jest/bin/jest.js --testPathIgnorePatterns=integration --forceExit",
    "test:integration": "node --localstorage-file=/tmp/agx-jest-localstorage ./node_modules/jest/bin/jest.js --testPathPattern=integration --forceExit",
    "test:all": "node --localstorage-file=/tmp/agx-jest-localstorage ./node_modules/jest/bin/jest.js --coverage --forceExit"
  },
  "keywords": [
    "ai",
    "agent",
    "cli",
    "gemini",
    "claude",
    "ollama",
    "llm",
    "cloud"
  ],
  "author": "Mendrika Ramarlina",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ramarlina/agx"
  },
  "dependencies": {
    "eventsource": "^4.1.0",
    "p-map": "^7.0.4",
    "p-retry": "^7.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "jest": "^30.2.0"
  }
}
